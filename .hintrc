{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 17.3",
    "not safari <= 17.3"
  ]
}